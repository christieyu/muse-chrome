{
	"name": "Muse",
	"version": "1.2.1",
	"author": "Christie Yu",
	"description": "A customizable homepage with a simple, minimalistic design.",
	"permissions": ["storage", "activeTab", "declarativeContent", "background", "geolocation"],
	"content_security_policy": "script-src 'self' https://example.com; object-src 'self'",
	"browser_action": {
		"default_icon": "/static/tab-icon.png",
		"default_title": "Have a good day",
		"default_popup": "/templates/popup.html"
	},
	"chrome_url_overrides" : {
		"newtab": "/templates/newtab.html"
	},
	"manifest_version": 2
}